{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch index.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceRoot}/index.html",
            "sourceMaps": true,
            "userDataDir": "${workspaceRoot}/../chromeTmp",
            "runtimeArgs": [ "--remote-debugging-port=9222"]
        }
    ]
}